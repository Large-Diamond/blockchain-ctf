pragma solidity ^0.8.13;

import {IPuzzle} from "../../general/IPuzzle.sol";

contract Lana is IPuzzle {
    mapping(uint256 => uint256) solvers;

    function name() external pure returns (string memory) {
        return "Lana";
    }

    function generate(address _seed) external pure returns (uint256) {
        return uint256(uint160(_seed));
    }

    function verify(uint256 start, uint256 _solution) external view returns (bool) {
        assert(solvers[start] != 0);
        return _solution == solvers[start];
    }

    function solve(bytes calldata val, address start) external {
        bytes memory data =
            hex"";

        assembly {
            let addr := create(0, add(data, 32), mload(data))
            let sz := extcodesize(addr)
            if iszero(sz) { revert(0, 0) }

            let code := mload(0x40)

            mstore(add(code, 0x14), start)
            calldatacopy(add(code, 0x40), val.offset, val.length)

            let success := call(gas(), addr, 0, add(code, 0x20), 0x100, code, 0x20)
            if or(iszero(eq(returndatasize(), 0x20)), iszero(success)) { revert(0, 0) }

            let secret := mload(code)
            if iszero(eq(secret, 0x1337)) { revert(0, 0) }
        }

        solvers[uint256(uint160(start))] = uint256(keccak256(val));
    }
}
